{
    "manifest_version": 2,
    "name": "SunFile",
    "description":
        "A small extemp utiliy that downloads an article in a word document format with publication names and dates.",
    "version": "3.0.0",
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "downloads",
        "storage",
        "cookies",
        "identity",
        "notifications"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "key":
        "",
    "externally_connectable": {
        "matches": ["http://localhost:3000/*", "https://sunfile-danielchao.rhcloud.com/*"]
    }
}
