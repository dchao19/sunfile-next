{
    "disabled": false,
    "scriptFile": "createFile.js",
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "route": "files/{fileId}",
            "methods": ["post"]
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        },
        {
            "type": "blob",
            "direction": "out",
            "name": "output",
            "path": "%environment%-files/{fileId}.docx",
            "dataType": "binary"
        }
    ]
}
